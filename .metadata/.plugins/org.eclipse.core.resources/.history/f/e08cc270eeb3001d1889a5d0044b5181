

create table member3(
	userid varchar2(8) primary key,
	userpwd varchar2(10) not null
)

select * from member3
insert into member3(userid, userpwd) values('candy', '1234');
insert into member3(userid, userpwd) values('admin', '5678');


create table bookshop3(
	isbn varchar2(15) primary key,
	title varchar2(50) not null,
	author varchar2(10) not null,
	company varchar2(50),
	price number
);

select * from bookshop3

insert into bookshop3(isbn, title, author, company, price) values('88-90-11', '오라클3일완성', '이오라', '야메루출판사', 15000);
insert into bookshop3(isbn, title, author, company, price) values('90-10-12', 'JSP달인되기', '송JP', '공갈닷컴', 20000);
insert into bookshop3(isbn, title, author, company, price) values('87-90-33', '자바무따기', '김자바', '디지탈박스', 35000);










